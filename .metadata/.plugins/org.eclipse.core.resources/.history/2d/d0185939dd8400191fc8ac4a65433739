package sistema_biblioteca;

import javax.swing.JLabel;
import javax.swing.JTextField;

public class Livro extends CadAcervo{
	
	

	public Livro(String Tipo) {
		super(Tipo);
		
		JLabel lblISBN = new JLabel("ISBN:");
		JLabel lblEdicao = new JLabel("Edição:");
		JLabel lblEditora = new JLabel("Editora:") ;
		
		JTextField txtISBN = new JTextField();
		JTextField txtEdicao = new JTextField();
		JTextField txtEditora = new JTextField();
		
		lblISBN.setBounds(5,5,100,30);
		txtISBN.setBounds(50,5,150,30);
		lblEdicao.setBounds(5,40,100,30);
		txtEdicao.setBounds(50,40,150,30);
		lblEditora.setBounds(5,80,100,30);
		txtEditora.setBounds(50,80,150,30);
		
		add(lblISBN);
		add(txtISBN);
		add(lblEdicao);
		add(txtEdicao);
		add(lblEditora);
		add(txtEditora);
	}

}
